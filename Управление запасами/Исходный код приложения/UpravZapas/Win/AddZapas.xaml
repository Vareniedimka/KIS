<Window x:Class="Win.AddZapas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Запасы" Height="163" Width="357" Icon="/Win;component/Images/phppg_3471.png" ResizeMode="NoResize">
    <Grid>
        <Button Content="Сохранить" Height="23" HorizontalAlignment="Left" Margin="76,95,0,0" Name="bSave" VerticalAlignment="Top" Width="75" Click="bSave_Click" />
        <Button Content="Отмена" Height="23" HorizontalAlignment="Left" Margin="187,95,0,0" Name="bCancel" VerticalAlignment="Top" Width="75" Click="bClose_Click" />
        <Label Content="Количество" Height="28" HorizontalAlignment="Left" Margin="20,50,0,0" Name="label4" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="99,51,0,0" Name="tbKolich" VerticalAlignment="Top" Width="240" MaxLength="200" KeyDown="tbKolich_KeyDown" />
        <Label Content="Материал" Height="28" HorizontalAlignment="Left" Margin="20,17,0,0" x:Name="label4_Copy" VerticalAlignment="Top" />
        <ComboBox HorizontalAlignment="Left" Margin="99,20,0,0" Name="cbMaterial" VerticalAlignment="Top" Width="240"/>
    </Grid>
</Window>
