<Window x:Class="Win.AddMaterialPostav"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Добавление матереала у поставщика" Height="151" Width="334" Icon="/Win;component/Images/phppg_3471.png" ResizeMode="NoResize">
    <Grid>
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="86,12,0,0" Name="cbMaterial" VerticalAlignment="Top" Width="230" />
        <Label Content="Материал" Height="28" HorizontalAlignment="Left" Margin="10,12,0,0" Name="label2" VerticalAlignment="Top" HorizontalContentAlignment="Center" />
        <Button Content="Сохранить" Height="23" HorizontalAlignment="Left" Margin="80,78,0,0" Name="bSave" VerticalAlignment="Top" Width="75" Click="bSave_Click" />
        <Button Content="Отмена" HorizontalAlignment="Right" Margin="0,78,80,0" Name="bCancel" Width="75" Height="23" VerticalAlignment="Top" Click="bCancel_Click" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="86,45,0,0" x:Name="cbPostavhik" VerticalAlignment="Top" Width="230" />
        <Label Content="Поставщик" Height="28" HorizontalAlignment="Left" Margin="10,45,0,0" x:Name="label2_Copy" VerticalAlignment="Top" HorizontalContentAlignment="Center" />
    </Grid>
</Window>
