        <Button Content="Отмена" Height="23" HorizontalAlignment="Left" Margin="179,229,0,0" Name="bCancel" VerticalAlignment="Top" Width="75" Click="bClose_Click" />
        <Label Content="Статус" Height="28" HorizontalAlignment="Left" Margin="86,89,0,0" Name="lb" VerticalAlignment="Top" />
        <Label Content="Материал" Height="28" HorizontalAlignment="Left" Margin="66,9,0,0" Name="label5" VerticalAlignment="Top" />
