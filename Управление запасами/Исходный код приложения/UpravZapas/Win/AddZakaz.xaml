        <TextBox Name="tbTimeVipoln" HorizontalAlignment="Left" Height="23" Margin="137,195,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="161"/>
        <Label Content="Дата оформления" Height="28" HorizontalAlignment="Left" Margin="22,118,0,0" x:Name="lb_Copy" VerticalAlignment="Top" />
        <Label Content="Время оформления" Height="28" HorizontalAlignment="Left" Margin="13,137,0,0" x:Name="lb_Copy1" VerticalAlignment="Top" />
        <Label Content="Дата выполнения" Height="28" HorizontalAlignment="Left" Margin="24,171,0,0" x:Name="lb_Copy2" VerticalAlignment="Top" />
        <Label Content="Время выполнения" Height="28" HorizontalAlignment="Left" Margin="15,190,0,0" x:Name="lb_Copy3" VerticalAlignment="Top" />
