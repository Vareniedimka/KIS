        <Button Content="Сохранить" Height="23" HorizontalAlignment="Left" Margin="16,402,0,0" Name="bSave" VerticalAlignment="Top" Width="75" Click="bSave_Click" />
        <TextBox Height="25" HorizontalAlignment="Left" Margin="246,350,0,0" Name="tbNaimenovanie_det" VerticalAlignment="Top" Width="178" />
